{
    "saveAndRun": {
        "commands": [
            {
                // Match against anything in the 'sakura' folder
                "match": "sakura\\\\.+\\.scss$",
                "cmd": "sass sakura:..\\."
            },
            {
                // Match against anything in the 'test' folder
                "match": "test\\\\.+\\.scss$",
                "cmd": "sass test:..\\."
            }
        ]
    }
}
